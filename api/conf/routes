GET        /games/new/               @controllers.EventStreamController.newGames(queryCondition: QueryCondition)
GET        /games/live/              @controllers.EventStreamController.liveGames(queryCondition: QueryCondition)

GET        /games/game/:id/          @controllers.GamesController.gameById(id: GameId)
GET        /games/                   @controllers.GamesController.gamesByIds(id: MultipleByIdQuery)

GET        /duels/raw/               @controllers.DuelsController.getAllGames(queryCondition: QueryCondition)
GET        /duels/recent/            @controllers.DuelsController.recentGames(queryCondition: QueryCondition, limit: LimitCondition)
GET        /duels/before/:id/        @controllers.DuelsController.gamesBefore(id: GameId, queryCondition: QueryCondition, limit: LimitCondition)

GET        /ctf/raw/                 @controllers.CtfController.getAllGames(queryCondition: QueryCondition)
GET        /ctf/recent/              @controllers.CtfController.recentGames(queryCondition: QueryCondition, limit: LimitCondition)
GET        /ctf/before/:id/          @controllers.CtfController.gamesBefore(id: GameId, queryCondition: QueryCondition, limit: LimitCondition)

GET        /nicknames/               @controllers.NicknamesController.getNicknames

GET        /version/                 controllers.VersionController.getVersion

GET        /clans/                   @controllers.ClansController.getClans
