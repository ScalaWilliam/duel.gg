{
  "id": "http://api.duel.gg/duel/schema.json#",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "required": [ "duration", "playedAt", "startTime", "map", "mode", "serverDescription", "server", "players", "endTime" ],
  "additionalProperties": true,
  "properties": {
    "duration": {"type": "integer"},
    "playedAt": {"type": "array", "minItems": 1, "items": { "type": "integer" }},
    "startTime": {"type": "string"},
    "endTime": {"type": "string"},
    "map": {"type": "string"},
    "mode": {"type": "string"},
    "serverDescription": {"type": "string"},
    "server": {"type": "string"},
    "winner": {"type": "string"},
    "demo": {"type": "string"},
    "tags": {"type": "array", "items": {"type": "string"}},
    "players": {
      "type": "array",
      "minItems": 2,
      "items": { "$ref": "#/definitions/player" }
    }
  },
  "definitions": {
    "player": {
      "required": ["name", "frags", "fragLog"],
      "properties": {
        "name": {"type": "string"},
        "frags": {"type": "integer"},
        "weapon": {"type": "string"},
        "ip": {"type": "string"},
        "accuracy": {"type": "integer"},
        "fragLog": {"type": "array", "items": {"type": "integer"}},
        "clan": {"type": "string"},
        "countryCode": {"type": "string"},
        "countryName": {"type": "string"}
      },
      "additionalProperties": true,
    }
  }
}
